{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "s3-upload",
  "title": "s3-upload",
  "description": "Upload an image or file to your S3 bucket to get a URL for this file.",
  "icon": "command-icon.png",
  "author": "sadovnikov",
  "categories": [
    "Productivity",
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Upload to S3",
      "description": "Upload an image or file to your S3 bucket to get a URL for this file.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.70.3"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}